{
  "name": "lichy",
  "version": "1.0.0",
  "description": "Lichy Container Management System",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd back-end && npm run dev",
    "frontend": "cd front-end && npm run dev",
    "install-deps": "npm install && cd back-end && npm install && cd ../front-end && npm install"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
